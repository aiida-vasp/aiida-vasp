{
    "#kpoints": "mesh for everythin up until wannier_setuppath for bands, format: [[\"A\", [...], \"B\", [...]], [...]]",
    "description": "Another Test of the vasp.windows workflow",
    "extras": {
        "experiment": "GaAs Tbmodel reproduction with multiple windows - Test",
        "tags": ["aiida-vasp", "workflow"],
        "workflow": "aiida.workflows.vasp.windows"
    },
    "kpoints": {
        "mesh": [8, 8, 8],
        "path": [
            ["X", [0.0, 0.5, 0.5], "G", [0.0, 0.0, 0.0]],
            ["G", [0.0, 0.0, 0.0], "L", [0.5, 0.5, 0.5]]
        ]
    },
    "label": "GaAs.sweeptest.0",
    "paw_family": "PBE",
    "paw_map": {
        "Ga": "Ga_d",
        "As": "As"
    },
    "projections": [
        "Ga : s; px; py; pz",
        "As : s; px; py; pz"
    ],
    "queue": "dphys_compute",
    "resources": {
        "num_machines": 4,
        "num_mpiprocs_per_machine": 6
    },
    "wannier_resources": {
        "num_machines": 1,
        "num_mpiprocs_per_machine": 1
    },
    "parameters": {
        "ediff": 1e-5,
        "encut": 280.00,
        "gga": "PE",
        "gga_compat": false,
        "ismear": 0,
        "lorbit": 11,
        "nbands": 24,
        "sigma": 0.05
    },
    "#parameters - not working with lwannier90?": {
        "npar": 6
    },
    "structure": "./GaAsTB/POSCAR",
    "vasp_code": "vasp-w90@monch", 
    "wannier_code": "wannier90.x@monch",
    "wannier_parameters": {
        "bands_plot": true,
        "hr_plot": true,
        "num_wann": 8,
        "num_bands": 24,
        "dis_num_iter": 100,
        "iprint": 2,
        "num_iter": 100,
        "use_bloch_phases": false
    },
    "windows": [
        {
            "inner": [0,  5],
            "outer": [-10, 15]
        } ,
        {
            "inner": [-10,  10],
            "outer": [-15, 35]
        },
        {
            "inner": [-5, 1],
            "outer": [-10, 25]
        },
        {
            "inner": [5, 10],
            "outer": [-10, 15]
        }
    ]
